<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name = "user"  verbose="4">
    <test name="企业账号密码登陆 login">
        <classes>
            <class name = "com.meiji.test.http.scrm.login.LoginTest"/>
        </classes>
    </test>

    <test name="扫码登录接口 loginscan">
        <classes>
            <class name="com.meiji.test.http.scrm.login.LoginScanTest"/>
        </classes>
    </test>

    <test name="二维码信息接口 qrcode">
        <classes>
            <class name="com.meiji.test.http.scrm.login.QrCodeTest"/>
        </classes>
    </test>

    <test name="菜单详情 menudetail">
        <classes>
            <class name="com.meiji.test.http.scrm.menu.MenuDetailTest"/>
        </classes>
    </test>

    <test name="菜单列表 menulist">
        <classes>
            <class name="com.meiji.test.http.scrm.menu.MenuListTest"/>
