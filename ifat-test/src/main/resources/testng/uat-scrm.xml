        <classes>
            <class name="com.meiji.test.http.scrm.corp.AgentConfigTest"/>
        </classes>
    </test>

    <test name="首页 index">
        <classes>
            <class name="com.meiji.test.http.scrm.corp.IndexTest"/>
        </classes>
    </test>

    <test name="客户列表 contactlist">
        <classes>
            <class name="com.meiji.test.http.scrm.contact.ContactListTest"/>
        </classes>
    </test>
